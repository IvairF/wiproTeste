@using Amil.AsmWebCad.UI.Resources;

<div id="DivisaoModal" class="modal fade in" data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="false">
    <div class="modal-dialog modal-sm" >
        <div class="modal-content">
            <div class="modal-header">
                Incuir Divisão da Unidade
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>

            <div class="modal-body">
                <form class="form-horizontal" onsubmit="return false;" id="frmDivisaoUnidade">
                    <div class="row">
                        <div class="col-sm-9">
                            <label class="control-label" id="lblDivisao">Divisão</label>
                            @Html.TextBox("Divisao", "", new { @class = "form-control", @maxlength = 15 })
                        </div>
                    </div>
                    <br />
                </form>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" title="@Resource.TituloBotaoSalvar" onclick="UnidadeDivisao.IncluirDivisao();">Incluir</button>
                    <button type="reset" class="btn btn-outline btn-primary" onclick="UnidadeDivisao.Cancelar();" title="@Resource.Cancelar">@Resource.Cancelar</button>
                </div>
            </div> 

        </div>
    </div>
</div>



