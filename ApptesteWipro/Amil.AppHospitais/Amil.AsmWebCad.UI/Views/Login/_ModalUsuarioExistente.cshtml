@using Amil.AsmWebCad.UI.Resources;
@model Amil.AsmWebCad.UI.Models.CadastraLoginModel

<div id="ExisteUsuarioModal" class="modal fade in" data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="false">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                @Resource.Erro
                <button type="button" class="close" data-dismiss="modal" aria-label="close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" onsubmit="return false;" id="frmDocumentoProfissional">
                    <div class="form-group">
                        <label class="col-lg-12 control-label" style="text-align : center">@Resource.ErroDuplicidade</label>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <div class="form-group" style="text-align : center">
                     @Html.ActionLink(@Resource.EsqueciMinhaSenha, "EsqueciSenha", "Login")
                </div>
            </div>

        </div>
    </div>
</div>
